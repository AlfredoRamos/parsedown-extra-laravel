{
	"name": "alfredo-ramos/parsedown-extra-laravel",
	"type": "package",
	"description": "A Parsedown Extra package for Laravel 5.3.x",
	"keywords": ["markdown extra", "parsedown extra", "laravel 5"],
	"homepage": "https://github.com/AlfredoRamos/parsedown-extra-laravel",
	"license": "GPL-3.0+",
	"authors": [
		{
			"name": "Alfredo Ramos",
			"email": "alfredo.ramos@yandex.com",
			"homepage": "https://github.com/AlfredoRamos",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.6.4",
		"erusev/parsedown-extra": "~0.7",
		"mews/purifier": "~2.0"
	},
	"require-dev": {
		"orchestra/testbench": "~3.3",
		"phpunit/phpunit": "~5.5"
	},
	"autoload": {
		"psr-4": {
			"AlfredoRamos\\ParsedownExtra\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"AlfredoRamos\\Tests\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.6-dev"
		}
	}
}
