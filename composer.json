{
	"name": "alfredo-ramos/parsedown-extra-laravel",
	"type": "package",
	"description": "A Parsedown Extra package for Laravel 5",
	"keywords": ["markdown extra", "parsedown extra", "laravel 5"],
	"homepage": "https://github.com/AlfredoRamos/parsedown-extra-laravel",
	"license": "GPL-3.0+",
	"authors": [
		{
			"name": "Alfredo Ramos",
			"email": "alfredo.ramos@yandex.com",
			"homepage": "https://github.com/AlfredoRamos",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.6.4",
		"erusev/parsedown-extra": "~0.7.0",
		"ezyang/htmlpurifier": "~4.9.0",
		"laravel/framework": "~5.4.0"
	},
	"require-dev": {
		"orchestra/testbench": "~3.4.0",
		"phpunit/phpunit": "~5.7.0"
	},
	"autoload": {
		"psr-4": {
			"AlfredoRamos\\ParsedownExtra\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"AlfredoRamos\\Tests\\": "tests/"
		}
	}
}
